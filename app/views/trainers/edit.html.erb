<div class="row">
  <div class="col s12">
    <h3>Edit A Trainer</h3>
  </div>
</div>
<div class="row">
  <div class="col s12">
    <%= form_for @trainer do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <br></br>
      <%= f.label :age %>
      <%= f.text_field :age %>
      <br></br>
      <%= f.submit %>
    <% end %>
    <%= button_to "Cancel", trainer_path(@trainer), method: :get %>
  </div>
</div>
