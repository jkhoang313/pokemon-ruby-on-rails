<div class="row">
  <div class="col s5">
    <%= image_tag("Blastoise.png", class: "med-image") %>
  </div>
  <div class="col s6 offset-s1">
    <h2>Login Page</h2>
    <br>
    <%= form_tag new_session_path do %>
      <%= label_tag :email %>
      <%= text_field_tag :email %>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
      <%= submit_tag "Login", class: "btn" %>
    <% end %>
  </div>
</div>
